  <% unless resource.persisted? %>
    <p><%= "Pour ajouter des dates, vous devez enregister ce spectable.".html_safe %></p>
  <% else %>
    <table id="seasons" class="table root">
      <thead>
      <tr>
        <th><%= t("simple_form.labels.rental_property_season.title") %></th>
        <th><%= t("simple_form.labels.rental_property_season.start_date") %></th>
        <th><%= t("simple_form.labels.rental_property_season.end_date") %></th>
        <th></th>
      </tr>
      </thead>
      <% f.object.spectacle_events.each do |spectacle_event| %>
        <%= render :partial => 'admin/spectacles/event', :locals => { :event => spectacle_event } %>
      <% end %>
    </table>
    <div class="well">
      <%#= btn_to(t("admin.rental_properties.actions.add_season"), 
        url_for([:new, :admin, current_site, @rental_property, :rental_property_season]), :remote => true) %>
    </div>
  <% end %> 